          "_name": "ampFeatures61",
          "_customHTML": "<section class=\"ampFeatureы61\" group=\"Features\" plugins=\"amp-main\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline=\"\" title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline=\"\" title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"checkbox\" title=\"Full Width:\" name=\"fullWidth\">\n        <!-- -->\n        <header>Title:</header>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Border:\" name=\"showBorderTitle\" condition=\"showTitle || showSubtitle\" checked=\"\">\n        <input type=\"color\" title=\"Color border:\" name=\"colorBorderTitle\" condition=\"showBorderTitle &amp;&amp; (showSubtitle || showTitle)\" value=\"#21b2f0\">\n        <select title=\"Align:\" name=\"alignTitle\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>Cards:</header>\n        <input type=\"range\" inline=\"\" title=\"Cards:\" name=\"cardsCount\" min=\"1\" max=\"6\" step=\"1\" value=\"6\" condition=\"showCardsTitle || showCardsSubtitle || checkImages\">\n        <input type=\"checkbox\" title=\"Border Cards:\" name=\"borderCards\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title:\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Border Title:\" name=\"showBorderTitleCard\" checked=\"\">\n        <input type=\"color\" title=\"Color Border Title:\" name=\"colorBorderTitleCard\" value=\"#21b2f0\" condition=\"showBorderTitleCard\">\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showCardsSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsTitle || showCardsSubtitle || checkImages\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#000000\" condition=\"!transparentBg &amp;&amp; (showCardsTitle || showCardsSubtitle || checkImages)\">\n        <input type=\"checkbox\" title=\"Images\" name=\"checkImages\" checked=\"\">\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#000000\" selected=\"\">\n            <input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/black%20pattern-2000x1333.jpg\">\n        </fieldset>\n        <!--  -->\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div mbr-class=\"{'container':!fullWidth, 'container-fluid':fullWidth}\">\n        <!-- Title -->\n        <div class=\"title-block mbr-pb-5\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n            <h3 class=\"mbr-section-title mbr-fonts-style mbr-pb-2 mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Balustrade Glass Fixing Methods and Design Ideas</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-1 mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\">fghj</h4>\n            <div class=\"line\" mbr-if=\"showBorderTitle &amp;&amp; (showTitle || showSubtitle)\">\n                <div class=\"line-line-gray\">\n                    <div class=\"line-line-color\"></div>\n                </div>\n            </div>\n        </div>\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showCardsTitle || showCardsSubtitle || checkImages\">\n            <!-- Card-1 -->\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-5': cardsCount == 1, 'mbr-col-md-6 mbr-col-lg-4': cardsCount >= 2, 'md-pb': cardsCount > 2, 'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-py-2\">\n                    <div class=\"card\">\n                        <div class=\"img\" mbr-if=\"checkImages\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/img-5398-785x586.jpeg\" layout=\"responsive\" width=\"392.65625\" height=\"294.021\" alt=\"Glass Handrails Auckland\" class=\"mobirise-loader\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"glass-balustrades.html#ampFooter3-af\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/img-5398-785x586.jpeg\" alt=\"Glass Handrails Auckland\"></a>\n                            </amp-img>\n                        </div>\n                        <div class=\"card-text mbr-py-3 mbr-px-4\" mbr-if=\"showCardsTitle || showCardsSubtitle\">\n                            <h6 mbr-text=\"\" class=\"card-subtitle mbr-fonts-style mbr-light mbr-pb-2\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle, .card-btn, .card-img\">Glass Patio Fence and Gates</h6>\n                            <h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title, .card-btn, .card-img\"><span style=\"font-weight: 400;\">Side Fixed Standoffs</span></h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Card-2 -->\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12': cardsCount == 1, 'mbr-col-md-6 mbr-col-lg-4': cardsCount >= 2, 'md-pb': cardsCount > 2, 'last-child': cardsCount == 1}\" mbr-if=\"cardsCount > 1\">\n                <div class=\"card-wrapper mbr-py-2\">\n                    <div class=\"card\">\n                        <div class=\"img\" mbr-if=\"checkImages\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/side-fixed-channel-frameless-balustrade.png\" layout=\"responsive\" width=\"392.65658364312264\" height=\"294.219\" alt=\"Glass Fencing Auckland\" class=\"mobirise-loader\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"glass-balustrades.html#ampFooter3-af\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/side-fixed-channel-frameless-balustrade.png\" alt=\"Glass Fencing Auckland\"></a>\n                            </amp-img>\n                        </div>\n                        <div class=\"card-text mbr-py-3 mbr-px-4\" mbr-if=\"showCardsTitle || showCardsSubtitle\">\n                            <h6 mbr-text=\"\" class=\"card-subtitle mbr-fonts-style mbr-light mbr-pb-2\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle, .card-btn, .card-img\">Deck Glass and Windbreak</h6>\n                            <h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title, .card-btn, .card-img\"><span style=\"font-weight: normal;\"><a href=\"glass-fencing-in-auckland.html\" class=\"text-secondary\" style=\"\">S</a>ide Fixed Channel</span></h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Card-3 -->\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12': cardsCount == 1, 'mbr-col-md-6 mbr-col-lg-4': cardsCount >= 2, 'md-pb': cardsCount > 2, 'last-child': cardsCount == 1}\" mbr-if=\"cardsCount > 2\">\n                <div class=\"card-wrapper mbr-py-2\">\n                    <div class=\"card\">\n                        <div class=\"img\" mbr-if=\"checkImages\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/side-fixed-mini-post-glass-balustrade-785x370.jpeg\" layout=\"responsive\" width=\"392.65560810810814\" height=\"294.99\" alt=\"Glass Railings Auckland\" class=\"mobirise-loader\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"glass-balustrades.html#ampFooter3-af\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/side-fixed-mini-post-glass-balustrade-785x370.jpeg\" alt=\"Glass Railings Auckland\"></a>\n                            </amp-img>\n                        </div>\n                        <div class=\"card-text mbr-py-3 mbr-px-4\" mbr-if=\"showCardsTitle || showCardsSubtitle\">\n                            <h6 mbr-text=\"\" class=\"card-subtitle mbr-fonts-style mbr-light mbr-pb-2\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle, .card-btn, .card-img\">Balcony Glass&nbsp;</h6>\n                            <h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title, .card-btn, .card-img\"><span style=\"font-weight: normal;\"><a href=\"glass-railings-in-auckland.html\" class=\"text-secondary\" style=\"\">S</a>ide Fixed Mini Posts</span></h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Card-4 -->\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-